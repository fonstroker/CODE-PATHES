<Window x:Class="Приложение.Window17"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window17" Background="#FFFC6A42" Height="495" Width="500" MaxHeight="495" MaxWidth="500" WindowStyle="None">
    <Grid>
        <Button x:Name="Back" Content="Назад" Margin="23,434,0,0" Background="White" BorderBrush="Black" FontFamily="Arial Black" Click="Back_Click" HorizontalAlignment="Left" Width="75" Height="23" VerticalAlignment="Top"/>
        <Button x:Name="StartT" Content="Начать тест" HorizontalAlignment="Center" Margin="0,200,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="36" Width="150" FontSize="18" Click="StartT_Click" RenderTransformOrigin="0.577,0.461"/>
        <TextBox x:Name="Q3" HorizontalAlignment="Left" Margin="45,237,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" FontFamily="Bahnschrift SemiBold" FontSize="14" Text="Как добавить элемент в List" Height="36" />
        <ComboBox x:Name="A2" HorizontalAlignment="Left" Margin="307,166,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="List&lt;int&gt; numbers = new List&lt;int&gt;();"/>
            <ComboBoxItem Content="new List&lt;int&gt;; numbers = new List&lt;int&gt;();"/>
            <ComboBoxItem Content="List&lt;int&gt; numbers = new List();"/>
            <ComboBoxItem Content="List&lt;int&gt; numbers = List&lt;int&gt;();"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A3" HorizontalAlignment="Left" Margin="307,243,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="list.Add(5);"/>
            <ComboBoxItem Content="list.Insert(0, 5);"/>
            <ComboBoxItem Content="list.AddElement(5);"/>
            <ComboBoxItem Content="list.InsertAtEnd(5);"/>
            <ComboBoxItem/>
        </ComboBox>
        <Button x:Name="Checked" Content="Проверить" HorizontalAlignment="Center" Margin="0,400,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="84" Click="Checked_Click" Visibility="Hidden"/>
        <Button x:Name="Backed" Content="Перейти к уровням" HorizontalAlignment="Center" Margin="0,414,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="32" Width="150" Click="Backed_Click" Visibility="Hidden"/>
        <TextBox x:Name="Q1" HorizontalAlignment="Left" Margin="45,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" Text="Какой из следующих типов данных не может использоваться в List" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="56" />
        <TextBox x:Name="Q2" HorizontalAlignment="Left" Margin="45,161,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" Text="Как создать новый List с типом int" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="38" RenderTransformOrigin="0.504,0.274" />
        <ComboBox x:Name="A1" HorizontalAlignment="Left" Margin="307,81,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="int"/>
            <ComboBoxItem Content="string"/>
            <ComboBoxItem Content="bool"/>
            <ComboBoxItem Content="double"/>
            <ComboBoxItem/>
        </ComboBox>
        <TextBox x:Name="Q4" HorizontalAlignment="Left" Margin="45,310,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" FontFamily="Bahnschrift SemiBold" FontSize="14" Text="Как удалить элемент из List по индексу" Height="36" />
        <ComboBox x:Name="A4" HorizontalAlignment="Left" Margin="307,314,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="list.Remove(0);"/>
            <ComboBoxItem Content="list.RemoveAt(0);"/>
            <ComboBoxItem Content="list.Delete(0);"/>
            <ComboBoxItem Content="list.Pop(0);"/>
            <ComboBoxItem/>
        </ComboBox>
        <Image x:Name="ImageTest" Source="/Тест.png" Margin="44,31,44,355"/>
    </Grid>
</Window>

        <ComboBox x:Name="A4" HorizontalAlignment="Left" Margin="325,343,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" RenderTransformOrigin="0.514,1.326" >

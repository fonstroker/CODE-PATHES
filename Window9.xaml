        <Button x:Name="Checked" Content="Проверить" HorizontalAlignment="Center" Margin="0,400,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="84" Click="Checked_Click" Visibility="Visible"/>

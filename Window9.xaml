        <TextBox x:Name="Q1" HorizontalAlignment="Left" Margin="16,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Text="string s = &quot;1103847330&quot;&#xA; foreach(char a in s)&#xA;{if (Convert.ToInt32(a)%2==0) {&#xA;Console.Write(a);&#xA;}}" FontFamily="Bahnschrift SemiBold" FontSize="14" />

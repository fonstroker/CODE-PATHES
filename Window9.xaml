        <TextBox x:Name="Q2" HorizontalAlignment="Left" Margin="16,127,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="300" Text="int a = 6;&#xD;&#xA;switch (a % 2)&#xD;&#xA;{case 0: Console.Write(&quot;Число четное&quot;); break;&#xD;&#xA; case 1: Console.Write(&quot;Число нечетное&quot;); break;}" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="90" />

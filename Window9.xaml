        <ComboBox x:Name="A1" HorizontalAlignment="Left" Margin="325,53,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="1100"/>
            <ComboBoxItem Content="3733"/>
            <ComboBoxItem Content="00"/>
            <ComboBoxItem Content="0840"/>
            <ComboBoxItem/>

<Window x:Class="Приложение.Window11"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window11" Background="#FFFC6A42" Height="495" Width="500" MaxHeight="495" MaxWidth="500" WindowStyle="None">
    <Grid>
        <Button x:Name="Back" Content="Назад" Margin="23,434,0,0" Background="White" BorderBrush="Black" FontFamily="Arial Black" Click="Back_Click" HorizontalAlignment="Left" Width="75" Height="23" VerticalAlignment="Top"/>
        <Button x:Name="StartT" Content="Начать тест" HorizontalAlignment="Center" Margin="0,203,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="36" Width="150" FontSize="18" Click="StartT_Click"/>
        <TextBox x:Name="Q3" HorizontalAlignment="Left" Margin="51,210,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" FontFamily="Bahnschrift SemiBold" FontSize="14" Text="За что отвечает функция Length для массива" Height="54" />
        <TextBox x:Name="Q4" HorizontalAlignment="Left" Margin="51,297,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" Text="Как в общей форме объявить двумерный массив 2 на 2" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="71" />
        <ComboBox x:Name="A2" HorizontalAlignment="Left" Margin="307,138,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="4"/>
            <ComboBoxItem Content="7"/>
            <ComboBoxItem Content="11"/>
            <ComboBoxItem Content="28"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A3" HorizontalAlignment="Left" Margin="307,222,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="За возвращение max элемента"/>
            <ComboBoxItem Content="За добавление элемента в массив"/>
            <ComboBoxItem Content="За возвращение длины массива"/>
            <ComboBoxItem Content="За счет суммы элементов массива"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A4" HorizontalAlignment="Left" Margin="307,319,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" RenderTransformOrigin="0.514,1.326" >
            <ComboBoxItem Content="int M = new int[2, 2];"/>
            <ComboBoxItem Content="int[2, 2] M = new int[2, 2];"/>
            <ComboBoxItem Content="int[,] M = new int[2, 2];"/>
            <ComboBoxItem Content="int[,,] M = new int[2, 2];"/>
            <ComboBoxItem/>
        </ComboBox>
        <Button x:Name="Checked" Content="Проверить" HorizontalAlignment="Center" Margin="0,400,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="84" Click="Checked_Click" Visibility="Visible"/>
        <Button x:Name="Backed" Content="Перейти к уровням" HorizontalAlignment="Center" Margin="0,414,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="32" Width="150" Click="Backed_Click" Visibility="Hidden"/>
        <TextBox x:Name="Q1" HorizontalAlignment="Left" Margin="51,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" Text="int[] M = { 4, 2, 3, 7, 8 };&#xA;Под каким индексом цифра 7?" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="59" />
        <TextBox x:Name="Q2" HorizontalAlignment="Left" Margin="51,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" Text="int[,] M = new int[4, 7];&#xA;Сколько столбцов в этом массиве?" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="55" />
        <ComboBox x:Name="A1" HorizontalAlignment="Left" Margin="307,49,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="7"/>
            <ComboBoxItem Content="4"/>
            <ComboBoxItem Content="04"/>
            <ComboBoxItem Content="3"/>
            <ComboBoxItem/>
        </ComboBox>
        <Image x:Name="ImageTest" Source="/Тест.png" Margin="44,31,44,355"/>
    </Grid>
</Window>

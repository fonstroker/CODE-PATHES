<Window x:Class="Приложение.Window13"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window13" Background="#FFFC6A42" Height="495" Width="500" MaxHeight="495" MaxWidth="500" WindowStyle="None">
    <Grid>
        <Button x:Name="Back" Content="Назад" Margin="23,434,0,0" Background="White" BorderBrush="Black" FontFamily="Arial Black" Click="Back_Click" HorizontalAlignment="Left" Width="75" Height="23" VerticalAlignment="Top"/>
        <Button x:Name="StartT" Content="Начать тест" HorizontalAlignment="Center" Margin="0,203,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="36" Width="150" FontSize="18" Click="StartT_Click"/>
        <TextBox x:Name="Q3" HorizontalAlignment="Left" Margin="23,285,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" FontFamily="Bahnschrift SemiBold" FontSize="14" Text="Какой тип возвращаемого значения необходимо поставить, чтобы функция ничего не возвращала" Height="75" />
        <ComboBox x:Name="A2" HorizontalAlignment="Left" Margin="307,169,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="a"/>
            <ComboBoxItem Content="float[]"/>
            <ComboBoxItem Content="float"/>
            <ComboBoxItem Content="a,b"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A3" HorizontalAlignment="Left" Margin="307,309,0,0" VerticalAlignment="Top" Width="158" Height="30" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="void"/>
            <ComboBoxItem Content="char[]"/>
            <ComboBoxItem Content="int"/>
            <ComboBoxItem Content="такого нет"/>
            <ComboBoxItem/>
        </ComboBox>
        <Button x:Name="Checked" Content="Проверить" HorizontalAlignment="Center" Margin="0,400,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="84" Click="Checked_Click" Visibility="Hidden"/>
        <Button x:Name="Backed" Content="Перейти к уровням" HorizontalAlignment="Center" Margin="0,414,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="32" Width="150" Click="Backed_Click" Visibility="Hidden"/>
        <TextBox x:Name="Q1" HorizontalAlignment="Left" Margin="23,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" Text="Что такое параметры?" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="30" />
        <TextBox x:Name="Q2" HorizontalAlignment="Left" Margin="23,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="186" Text="Какой тип возвращаемого значения соответствует данной функции:&#xD;&#xA;public static float[] Point(float a, float b)&#xD;&#xA;{&#xD;&#xA;    return new float[] { a, b };&#xD;&#xA;}" FontFamily="Bahnschrift SemiBold" FontSize="14" Height="140" />
        <ComboBox x:Name="A1" HorizontalAlignment="Left" Margin="307,49,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="Размер функции"/>
            <ComboBoxItem Content="Количества повторений функции"/>
            <ComboBoxItem Content="Время работы функции"/>
            <ComboBoxItem Content="Значения, которые принимает функция"/>
            <ComboBoxItem/>
        </ComboBox>
        <Image x:Name="ImageTest" Source="/Тест.png" Margin="44,31,44,355"/>
    </Grid>
</Window>

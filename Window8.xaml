<Window x:Class="Приложение.Window8"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window8" Height="495" Width="500" WindowStyle="None" Background="#FFFC6A42">
    <Grid>
        <TextBox x:Name="L3Text" HorizontalAlignment="Center" Visibility="Hidden" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="392" Height="155" FontFamily="Bahnschrift SemiBold" FontSize="16" />
        <Button x:Name="Start" Content="Начать" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Arial Black" FontSize="25" BorderBrush="#FFFF6237" Background="White" Height="49" Width="144" Click="Start_Click"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="24,442,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="23" Width="75" Click="Back_Click"/>
        <Button x:Name="NextT" Content="Далее" Visibility="Hidden" HorizontalAlignment="Left" Margin="250,367,0,0" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="14" BorderBrush="#FFFF6237" Background="White" Height="28" Width="82" Click="NextT_Click"/>
        <Button x:Name="BackT" Content="Назад" HorizontalAlignment="Left" Margin="155,367,0,0" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="14" BorderBrush="#FFFF6237" Background="White" Height="28" Width="82" Click="BackT_Click" Visibility="Hidden"  />
        <Button x:Name="Test" Content="Проверить знания" HorizontalAlignment="Center" Margin="0,411,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="31" Width="160" FontSize="14" Click="Test_Click" Visibility="Hidden" />
        <Image x:Name="ImaInstrum" HorizontalAlignment="Center" Height="129" Margin="0,26,0,0" VerticalAlignment="Top" Width="316" Source="/Операторы ветвления и циклы.png" />
        <Image x:Name="image1" Height="183" Margin="41,177,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="289" VerticalAlignment="Top" Source="/Напримерif.png" />
        <Image x:Name="Image2" Margin="338,260,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="122" Source="/ЧислаНеРавны.png" Height="28" VerticalAlignment="Top"/>
        <Image x:Name="Image3" HorizontalAlignment="Center" Height="136" VerticalAlignment="Top" Width="436"  Visibility="Hidden" Source="/НапримерSwitch.png" Margin="0,186,0,0"/>
        <Image x:Name="Image4"  Margin="0,330,0,0" Visibility="Hidden" Source="/ПриветТом.png" HorizontalAlignment="Center" Width="122" Height="65" VerticalAlignment="Top"/>
        <Image x:Name="Image5" Height="123" Width="230" Margin="82,193,0,0" Visibility="Hidden" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/НапримерWhile.png"/>
        <Image x:Name="Image6"  Margin="349,188,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="24" Height="138" VerticalAlignment="Top" Source="/РезультатWhile.png"/>
        <Image x:Name="Image7" Margin="82,201,0,0"  Visibility="Hidden" HorizontalAlignment="Left" Width="245" Height="107" VerticalAlignment="Top" Source="/НапримерWhDo.png"/>
        <Image x:Name="Image8"  Margin="349,239,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="26" Height="32" VerticalAlignment="Top" Source="/Результат WhDo.png"/>
        <Image x:Name="Image9" Height="102" Width="284"  Margin="82,202,0,0" Visibility="Hidden" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Напримерfor.png"/>
        <Image x:Name="Image10" Margin="387,193,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="26" Height="120" VerticalAlignment="Top" Source="/РезультатFor.png"/>
        <Image x:Name="Image11" Height="115" Width="253" Margin="77,195,0,0" Visibility="Hidden" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Напримерforeach.png"/>
        <Image x:Name="Image12"  Margin="342,191,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="24" Height="135" VerticalAlignment="Top" Source="/Результатforach.png"/>
        <Button x:Name="Interac" Content="Интерактив" HorizontalAlignment="Center" Margin="0,408,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="140" FontSize="14" Visibility="Hidden" Click="Interac_Click"/>
        <TextBox x:Name="PlaceIF" HorizontalAlignment="Left" Margin="12,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Height="54" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Foreground="Black" />
        <TextBox x:Name="PlaceT" HorizontalAlignment="Left" Margin="12,277,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Height="54" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Foreground="Black" />
        <TextBox x:Name="PlaceF" HorizontalAlignment="Left" Margin="12,376,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Height="54" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Foreground="Black" />
        <Button x:Name="Check" Content="Проверить" HorizontalAlignment="Left" Margin="320,242,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="118" FontSize="14" Visibility="Hidden" Click="Check_Click"/>
        <Button x:Name="back1" Content="Вернуться" HorizontalAlignment="Left" Margin="24,442,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="22" Width="75" Click="back1_Click" Visibility="Hidden"/>
        <Label x:Name="TeIF" Content="Место проверки (if без {})" HorizontalAlignment="Left" Margin="12,142,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <Label x:Name="TeT" Content="Место для действий при true" HorizontalAlignment="Left" Margin="12,242,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <Label x:Name="TeF" Content="Место для действий при false" HorizontalAlignment="Left" Margin="12,341,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <TextBox x:Name="TextCat" HorizontalAlignment="Left" Margin="12,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="249" Height="101" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Text="Проверьте, хочет ли кот кушать или нет. Если хочет, то научите его говорить &quot;Хочу кушать&quot;, иначе &quot;Не хочу кушать&quot;. (Ответ кота - это переменная A)"/>
        <ComboBox x:Name="Val" HorizontalAlignment="Left" Margin="317,201,0,0" VerticalAlignment="Top" Width="124" Height="29" FontFamily="Bahnschrift SemiBold" Visibility="Hidden" >
            <ComboBoxItem Content="Хочу кушать"/>
            <ComboBoxItem Content="Не хочу кушать"/>
            <ComboBoxItem/>
        </ComboBox>
        <Image x:Name="K1"  Margin="268,53,10,305" Source="/Котик-вопрос.png" Visibility="Hidden"/>
        <Image x:Name="K2"  Margin="268,49,9,306"  Visibility="Hidden" Source="/Котик-хочу кушать.png" />
        <Image x:Name="K3"  Margin="268,50,10,306"  Visibility="Hidden" Source="/Котик- не хочу кушать.png" />
        <Button x:Name="Interac1" Content="Интерактив" HorizontalAlignment="Center" Margin="0,408,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="140" FontSize="14" Visibility="Hidden" Click="Interac1_Click"/>
        <TextBox x:Name="PlaceFOR" HorizontalAlignment="Left" Margin="24,236,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Height="54" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Foreground="Black" />
        <TextBox x:Name="PlaceBody" HorizontalAlignment="Left" Margin="24,336,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" Height="54" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Foreground="Black" />
        <Button x:Name="Calcul" Content="Посчитать" HorizontalAlignment="Left" Margin="340,267,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="118" FontSize="14" Visibility="Hidden" Click="Calcul_Click"/>
        <Button x:Name="back2" Content="Вернуться" HorizontalAlignment="Left" Margin="24,442,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="22" Width="75" Visibility="Hidden" Click="back2_Click"/>
        <Label x:Name="TeFOR" Content="Место для цикла for" HorizontalAlignment="Left" Margin="24,201,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <Label x:Name="TeBody" Content="Место для тела цикла for" HorizontalAlignment="Left" Margin="24,301,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <TextBox x:Name="TextCalcul" HorizontalAlignment="Left" Margin="24,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="249" Height="136" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Text="Помогите роботу!&#xD;&#xA;Помогите роботу создать таблицу умножения с помощью цикла for (i - параметр). Начальная цифра - K. Все переменные уже объявлены, умножение начиная с 1"/>
        <ComboBox x:Name="ValueCalc" HorizontalAlignment="Left" Margin="337,226,0,0" VerticalAlignment="Top" Width="124" Height="29" FontFamily="Bahnschrift SemiBold" Visibility="Hidden" >
            <ComboBoxItem Content="1"/>
            <ComboBoxItem Content="2"/>
            <ComboBoxItem Content="3"/>
            <ComboBoxItem Content="4"/>
            <ComboBoxItem Content="5"/>
            <ComboBoxItem Content="6"/>
            <ComboBoxItem Content="7"/>
            <ComboBoxItem Content="8"/>
            <ComboBoxItem Content="9"/>
            <ComboBoxItem/>
        </ComboBox>
        <TextBox x:Name="PlaceDigat" HorizontalAlignment="Left" Margin="291,16,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="29" Height="181" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Foreground="Black" />
        <Image x:Name="Rob2"  Margin="299,-12,-14,293" Source="/Робот-таблица умн.png" Visibility="Hidden"/>
        <Image x:Name="Rob1" Source="/Робот-таблица умн до.png" Margin="300,-12,-15,291" Visibility="Hidden"/>
    </Grid>
</Window>

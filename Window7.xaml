<Window x:Class="Приложение.Window7"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window7" Height="495" Width="500" WindowStyle="None">
    <Grid Background="#FFFC6A42" Height="495" Width="500" MaxHeight="495" MaxWidth="500" >
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="23,434,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="23" Width="75" Click="Back_Click"/>
        <Button x:Name="StartT" Content="Начать тест" HorizontalAlignment="Center" Margin="0,203,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="36" Width="150" FontSize="18" Click="StartT_Click"/>
        <TextBox x:Name="Q2" HorizontalAlignment="Left" Margin="45,138,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" Text="За что отвечает логическая операция &amp;&amp;" FontFamily="Bahnschrift SemiBold" FontSize="15" />
        <TextBox x:Name="Q3" HorizontalAlignment="Left" Margin="45,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" Text="Как переменную a умножить на 6 составным присвоением" FontFamily="Bahnschrift SemiBold" FontSize="15" />
        <TextBox x:Name="Q4" HorizontalAlignment="Left" Margin="45,323,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" Text="bool s = 5 &lt; 3 || 0 &gt; -2; &#xD;&#xA;Что будет в консоли при выводе в нее s" FontFamily="Bahnschrift SemiBold" FontSize="15" />
        <TextBox x:Name="Q1" HorizontalAlignment="Left" Margin="45,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="183" Text="Как правильно считать значение вещественной переменной с консоли" FontFamily="Bahnschrift SemiBold" FontSize="15" />
        <ComboBox x:Name="A1" HorizontalAlignment="Left" Margin="270,52,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="double a = Convert.ToDouble(Console.ReadLine());"/>
            <ComboBoxItem Content="Double a = Convert.ToDouble(ConsoleReadLine);"/>
            <ComboBoxItem Content="double a = ConvertToDouble(ConsoleReadLine());"/>
            <ComboBoxItem Content="Double a = Convert.ToDouble(Console.ReadLine());"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A2" HorizontalAlignment="Left" Margin="270,144,0,0" VerticalAlignment="Top" Width="158" Height="28" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="Логическая или"/>
            <ComboBoxItem Content="Логическая не"/>
            <ComboBoxItem Content="Логическая и"/>
            <ComboBoxItem Content="Логическая исключающая или"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A3" HorizontalAlignment="Left" Margin="270,0,0,0" VerticalAlignment="Center" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="a = a * 6;"/>
            <ComboBoxItem Content="a *= 6;"/>
            <ComboBoxItem Content="a = a + a + a + a + a + a;"/>
            <ComboBoxItem Content="a+=6;"/>
            <ComboBoxItem/>
        </ComboBox>
        <ComboBox x:Name="A4" HorizontalAlignment="Left" Margin="270,337,0,0" VerticalAlignment="Top" Width="158" Height="29" FontFamily="Bahnschrift SemiBold" >
            <ComboBoxItem Content="True"/>
            <ComboBoxItem Content="False"/>
            <ComboBoxItem/>
        </ComboBox>
        <Button x:Name="Checked" Content="Проверить" HorizontalAlignment="Center" Margin="0,400,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="84" Visibility="Visible" Click="Checked_Click"/>
        <Button x:Name="Backed" Content="Перейти к уровням" HorizontalAlignment="Center" Margin="0,414,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="32" Width="150" Visibility="Hidden" Click="Backed_Click"/>
        <Image x:Name="ImageTest" Source="/Тест.png" Margin="44,31,44,355"/>
    </Grid>
</Window>

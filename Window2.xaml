<Window x:Class="Приложение.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window2" Height="495" Width="400" WindowStyle="None" MaxHeight="495" MaxWidth="400">
    <Grid Background="#FFFC6A42" MaxHeight="495" MaxWidth="400" HorizontalAlignment="Center" Width="400" Height="495" VerticalAlignment="Top">
        <Button x:Name="Variables" Content="Переменные и&#xD;&#xA; типы данных" HorizontalAlignment="Left" Margin="35,166,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="Variables_Click"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="23,434,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="23" Width="75" Click="Back_Click"/>
        <Button x:Name="Operations" Content="       Операторы &#xD;&#xA;ветвления и циклы" HorizontalAlignment="Left" Margin="35,221,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="Operations_Click"/>
        <Button x:Name="InstrumWork" Content="Основные инстру-&#xA;менты и операции" HorizontalAlignment="Left" Margin="200,166,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="InstrumWork_Click"/>
        <Button x:Name="Profile" Content="Профиль" HorizontalAlignment="Left" Margin="305,10,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="22" Width="75" Click="Profile_Click"/>
        <Image Source="/Темы для изучения.png" Margin="0,37,0,0" HorizontalAlignment="Center" Width="312" Height="82" VerticalAlignment="Top"/>
        <Button x:Name="Mass" Content="Массивы" HorizontalAlignment="Left" Margin="200,221,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="Mass_Click"/>
        <Button x:Name="Function" Content="Функции" HorizontalAlignment="Left" Margin="35,277,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="Function_Click"/>
        <Button x:Name="Faile" Content="Файлы" HorizontalAlignment="Left" Margin="200,277,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="Faile_Click"/>
        <Button x:Name="ListL" Content="Листы" HorizontalAlignment="Left" Margin="35,333,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="ListL_Click"/>
        <Button x:Name="tryW" Content="  Обработка&#xA;исключений" HorizontalAlignment="Left" Margin="200,333,0,0" VerticalAlignment="Top" BorderBrush="#FFFF6237" Background="White" Height="42" Width="159" FontFamily="Arial Black" FontSize="14" Click="tryW_Click"/>
    </Grid>
</Window>

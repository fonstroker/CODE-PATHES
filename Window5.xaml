<Window x:Class="Приложение.Window5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Приложение"
        mc:Ignorable="d"
        Title="Window5" Height="495" Width="500" WindowStyle="None" Background="#FFFC6A42">
    <Grid >
        <TextBox x:Name="L2Text" HorizontalAlignment="Center" Margin="0,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="392" Height="155" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden"/>
        <Button x:Name="Start" Content="Начать" HorizontalAlignment="Center" Margin="0,219,0,0" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="25" BorderBrush="#FFFF6237" Background="White" Height="49" Width="145" Click="Start_Click"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="24,442,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="23" Width="75" Click="Back_Click"/>
        <Button x:Name="NextT" Content="Далее" HorizontalAlignment="Left" Margin="250,367,0,0" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="14" BorderBrush="#FFFF6237" Background="White" Height="28" Width="82" Click="NextT_Click" Visibility="Hidden"   />
        <Button x:Name="BackT" Content="Назад" HorizontalAlignment="Left" Margin="155,367,0,0" VerticalAlignment="Top" FontFamily="Arial Black" FontSize="14" BorderBrush="#FFFF6237" Background="White" Height="28" Width="82" Click="BackT_Click" Visibility="Hidden"  />
        <Button x:Name="Test" Content="Проверить знания" HorizontalAlignment="Center" Margin="315,429,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="31" Width="160" FontSize="14" Click="Test_Click" Visibility="Hidden" />
        <Image x:Name="ImaInstrum" HorizontalAlignment="Left" Height="120" Margin="46,16,0,0" VerticalAlignment="Top" Width="400" Source="/Основные инструменты и операции.png" />
        <Image x:Name="image1" Height="96" Source="/НапримерConsoleWrite.png" Margin="106,196,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="294" VerticalAlignment="Top"/>
        <Image x:Name="Image2" Margin="0,307,0,0" Source="/Рез-татConsoleWr.png" Visibility="Hidden" HorizontalAlignment="Center" Width="162" Height="50" VerticalAlignment="Top"/>
        <Image x:Name="Image3" HorizontalAlignment="Center" Height="73" Margin="0,208,0,0" VerticalAlignment="Top" Width="446" Source="/НапримерReadLine.png" Visibility="Hidden"/>
        <Image x:Name="Image4" Source="/НапримерАрифмОп.png" Margin="79,197,157,157" Visibility="Hidden"/>
        <Image x:Name="Image5" Height="96" Width="29" Source="/РезультатАрифм.png" Margin="366,224,0,0" Visibility="Hidden" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Image x:Name="Image6" Source="/НапримерУнарные.png" Margin="130,186,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="175" Height="174" VerticalAlignment="Top"/>
        <Image x:Name="Image7" Margin="320,241,0,0" Source="/РезультатУнарные.png" Visibility="Hidden" HorizontalAlignment="Left" Width="32" Height="78" VerticalAlignment="Top"/>
        <Image x:Name="Image8" Source="/Снимок экрана 2024-05-25 212938.png" Margin="74,206,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="256" Height="148" VerticalAlignment="Top"/>
        <Image x:Name="Image9" Height="71" Width="59" Source="/Снимок экрана 2024-05-25 213146.png" Margin="343,248,0,0" Visibility="Hidden" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Image x:Name="Image10" Source="/Снимок экрана 2024-05-25 214415.png" Margin="68,197,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="256" Height="150" VerticalAlignment="Top"/>
        <Image x:Name="Image11" Height="73" Width="64" Source="/Снимок экрана 2024-05-25 214716.png" Margin="341,237,0,0" Visibility="Hidden" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Image x:Name="Image12" Source="/Снимок экрана 2024-05-25 220505.png" Margin="112,193,0,0" Visibility="Hidden" HorizontalAlignment="Left" Width="198" Height="166" VerticalAlignment="Top"/>
        <Image x:Name="Image13" Height="71" Width="34" Source="/Снимок экрана 2024-05-25 220622.png" Margin="331,241,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Visibility="Hidden"/>
        <TextBox x:Name="TextInter" HorizontalAlignment="Left" Margin="37,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="119" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Text="Помогите роботу!&#xA;Научите робота говорить: &quot;Привет мир!&quot; с помощью специального метода&#xD;&#xA;Console.Write(); или&#xD;&#xA;Console.WriteLine();"/>
        <TextBox x:Name="PlaceConsole" HorizontalAlignment="Left" Margin="37,246,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="56" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden"/>
        <Label x:Name="TeConsole" Content="Место для вывода строки" HorizontalAlignment="Left" Margin="37,210,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <Image x:Name="Rob1" Margin="158,-26,-13,166"  Visibility="Hidden" Source="/Робот привет мир.png" />
        <Image x:Name="Rob2" Margin="161,-24,-10,169" Source="/Робот не привет мир.png" Visibility="Hidden" />
        <Button x:Name="Study" Content="Научить" HorizontalAlignment="Left" Margin="326,363,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="108" FontSize="14" Visibility="Hidden" Click="Check1_Click"/>
        <Button x:Name="Interac" Content="Интерактив" HorizontalAlignment="Center" Margin="0,405,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="140" FontSize="14" Visibility="Hidden" Click="Interac_Click"/>
        <Button x:Name="back1" Content="Вернуться" HorizontalAlignment="Left" Margin="24,442,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="22" Width="75" Click="back1_Click" Visibility="Hidden"/>
        <Label x:Name="TeC" Content="Место для проверки" HorizontalAlignment="Left" Margin="37,263,0,0" VerticalAlignment="Top" Background="White" FontFamily="Arial Black" Visibility="Hidden" FontSize="14"/>
        <Image x:Name="Kos2" Source="/огонь горит.png" Margin="297,50,38,287" Visibility="Hidden"/>
        <Image x:Name="Kos1" Source="/огонь не горит.png" Margin="288,54,30,287" Visibility="Hidden" />
        <TextBox x:Name="TextInter1" HorizontalAlignment="Left" Margin="37,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="200" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden" Text="Помогите разжечь костер! &#xA;Перед тем как забросить бревна в костер (K), необходимо проверить (C - проверка (bool)), сухие ли они (K - сухие (S)) и не металлические (K - не металлические (M)). Все переменные уже объявлены!"/>
        <TextBox x:Name="PlaceCheck" HorizontalAlignment="Left" Margin="37,302,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="209" Height="56" FontFamily="Bahnschrift SemiBold" FontSize="16" Visibility="Hidden"/>
        <Button x:Name="Check" Content="Проверить" HorizontalAlignment="Left" Margin="326,237,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="28" Width="108" FontSize="14" Visibility="Hidden" Click="Check_Click"/>
        <Button x:Name="back2" Content="Вернуться" HorizontalAlignment="Left" Margin="24,442,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="22" Width="75" Visibility="Hidden" Click="back2_Click"/>
        <Button x:Name="Interac1" Content="Интерактив" HorizontalAlignment="Center" Margin="0,405,0,0" VerticalAlignment="Top" Background="White" BorderBrush="Black" FontFamily="Arial Black" Height="27" Width="140" FontSize="14" Visibility="Hidden" Click="Interac1_Click"/>
    </Grid>
</Window>
